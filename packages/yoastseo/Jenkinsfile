podTemplate(
	containers: [
		containerTemplate(
			name: 'node',
			image: 'node:12',
			ttyEnabled: true,
			envVars: []
		)
	],
	volumes: [
   		emptyDirVolume(mountPath: '/var/www/html', memory: false),
	]
) {
	node(POD_LABEL) {
		stage('Run shell') {
			container('node') {
				sh 'yarn'
			}
		}
	}
}
